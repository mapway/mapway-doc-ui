<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:cn.mapway.document.ui.client.main">
	<ui:style>
        .none{
	padding: 20px 0px;
	font-weight: bold;
	color: green;
	text-align: left;
}
.url{
	padding: 15px 0px;
}
	</ui:style>
	<g:FlowPanel>
		<g:Label styleName="{style.none}" ui:field="lbNone">无需传入参数</g:Label>
		<g:Label ui:field="lbUrl" styleName="{style.url}"></g:Label>
		<m:ObjectsInfoPanel ui:field="paraPath"></m:ObjectsInfoPanel>
		<m:ObjectsInfoPanel ui:field="paraQuery"></m:ObjectsInfoPanel>
		
		<m:ParameterPanel ui:field="paraBody"></m:ParameterPanel>
	</g:FlowPanel>
			
</ui:UiBinder> 